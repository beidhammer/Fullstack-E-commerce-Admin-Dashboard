<%- include('partials/header', { user, token }) %>
<h1>Role Management</h1>
<table border="1">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% roles.forEach(r => { %>
      <tr>
        <td><%= r.id %></td>
        <td><%= r.name %></td>
      </tr>
    <% }) %>
  </tbody>
</table>